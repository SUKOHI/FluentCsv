{
    "name": "sukohi/fluent-csv",
    "description": "A Laravel package to generate or save csv file with encoding like SJIS-win.",
    "authors": [
        {
            "name": "Sukohi",
            "email": "capilano.sukohi@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "illuminate/support": "^5.7|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Sukohi\\FluentCsv\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Sukohi\\FluentCsv\\FluentCsvServiceProvider"
            ],
            "aliases": {
                "FluentCsv": "Sukohi\\FluentCsv\\Facades\\FluentCsv"
            }
        }
    }
}
